{
    "name": "vue-sample",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint . --fix",
        "format": "prettier --write src/",
        "generate-api": "npx @openapitools/openapi-generator-cli generate -i http://localhost:8081/v3/api-docs -g typescript -o ./src/generate-api",
        "generate-api-axios": "npx @openapitools/openapi-generator-cli generate -i http://localhost:8081/v3/api-docs -g typescript-axios -o ./src/generate-api-axios",
        "generate-api-fetch": "npx @openapitools/openapi-generator-cli generate -i http://localhost:8081/v3/api-docs -g typescript-fetch -o ./src/generate-api-fetch"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.23",
        "@intlify/unplugin-vue-i18n": "^6.0.0",
        "@openapitools/openapi-generator-cli": "^2.15.3",
        "@vueuse/core": "^11.2.0",
        "ant-design-vue": "4.x",
        "date-fns": "^4.1.0",
        "dayjs": "^1.11.13",
        "petite-vue-i18n": "^10.0.4",
        "pinia": "^2.2.4",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "simplebar-vue": "^2.3.5",
        "vue": "^3.5.12",
        "vue-i18n": "^10.0.4",
        "vue-router": "4",
        "whatwg-fetch": "^3.6.20"
    },
    "devDependencies": {
        "@tsconfig/node20": "^20.1.4",
        "@types/node": "^20.16.11",
        "@vitejs/plugin-vue": "^5.1.4",
        "@vue/eslint-config-prettier": "^10.0.0",
        "@vue/eslint-config-typescript": "^14.0.1",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.12.0",
        "eslint-plugin-vue": "^9.29.0",
        "npm-run-all2": "^6.2.3",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.14",
        "typescript": "~5.5.4",
        "unplugin-vue-components": "^0.27.4",
        "vite": "^5.4.8",
        "vite-plugin-vue-devtools": "^7.6.4",
        "vue-tsc": "^2.1.6"
    }
}